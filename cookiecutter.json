{
  "project_name": "Azure Web App Project",
  "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '')|replace('-', '')|replace('.', '')|trim() }}",
  "project_suffix": "{{ random_ascii_string(5).lower() }}",
  "description": "Behold Azure Web App Project!",
  "author_name": "PainKiller995",
  "domain_name": ".azurewebsites.net",
  "email": "{{ cookiecutter.author_name.lower() | trim() |replace(' ', '-') }}@{{ cookiecutter.domain_name.lower() | trim() }}",
  "version": "0.1.0",
  "open_source_license": [
    "MIT",
    "BSD",
    "GPLv3",
    "Apache Software License 2.0",
    "Not open source"
  ],
  "timezone": "UTC",
  "postgresql_version": ["15", "14", "13", "12", "11", "10"],
  "use_celery": "n",
  "use_whitenoise": "y",
  "azure_location": "eastus",
  "azure_res_group_name": "{{''.join([cookiecutter.project_slug, 'group', cookiecutter.project_suffix])}}",
  "azure_container_registry_name": "{{''.join([cookiecutter.project_slug, 'registry', cookiecutter.project_suffix])}}",
  "azure_app_service_plan": "{{''.join([cookiecutter.project_slug, 'sp', cookiecutter.project_suffix])}}",
  "azure_web_app_name": "{{''.join([cookiecutter.project_slug, 'webapp', cookiecutter.project_suffix])}}",
  "azure_postgres_host": "{{''.join([cookiecutter.project_slug, 'ph', cookiecutter.project_suffix])}}",
  "azure_postgres_database": "{{''.join([cookiecutter.project_slug, 'db', cookiecutter.project_suffix])}}",
  "azure_postgres_username": "postgresuser",
  "azure_postgres_password": "123456789",
  "debug": "n"
}
